  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Example",
      "program": "${workspaceFolder}/example/index.js",
      "args": [
        "hello",
        "--name",
        "mike"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Default",
      "program": "${workspaceFolder}/index.js",
      "args": [
        "make:command",
        "TestCommand",
        "--name",
        "mike"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Default w/Help",
      "program": "${workspaceFolder}/index.js",
      "args": [
        "--help"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Current File",
      "program": "${file}"
    }
  ]
