{
  "name": "@codedungeon/gunner",
  "packageName": "gunner",
  "version": "0.10.8",
  "build": "198",
  "description": "Node CLI Framework",
  "tagline": "Crafted with love by Codedungeon",
  "author": "Mike Erickson <codedungeon@gmail.com> (https://github.com/mikeerickson)",
  "repository": "https://github.com/mikeerickson/gunner",
  "contributors": [
    {
      "name": "Mike Erickson",
      "email": "mike.erickson@codedungeon.io",
      "url": "https://github.com/mikeerickson"
    }
  ],
  "main": "index.js",
  "bin": {
    "gunner": "index.js"
  },
  "engines": {
    "node": ">=10.0.0 <=15.7.0"
  },
  "scripts": {
    "bump": "./tasks/bumpBuild.js",
    "bump:dev": "./tasks/bumpBuild.js --verbose",
    "lint": "./tasks/lint.sh",
    "test": "./tasks/test.js $1",
    "test:all": "mocha ./test/*.test.js && node ./test/utils/testCleanup.js",
    "test:ci": "mocha --reporter dot",
    "test:spec": "mocha --reporter spec",
    "test:commit": "mocha ./test/*.test.js --reporter dot  && node ./test/utils/testCleanup.js",
    "test:coverage": "nyc mocha ./test/*.test.js",
    "test:watch": "mocha ./test/*.test.js --growl --watch --reporter progress",
    "todo": "node tasks/todo.js",
    "pre-commit": "npm run lint && npm run test:commit"
  },
  "keywords": [
    "cli",
    "codedungeon",
    "framework",
    "javascript",
    "node",
    "starter",
    "skeleton"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "leasot": "11.5.0",
    "mocha-better-spec-reporter": "3.1.0",
    "mock-fs": "4.13.0",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "sinon": "9.2.4"
  },
  "dependencies": {
    "@codedungeon/messenger": "0.9.1",
    "@codedungeon/utils": "0.0.12",
    "apisauce": "2.0.1",
    "chalk": "4.1.0",
    "cli-table3": "0.6.0",
    "configstore": "5.0.1",
    "dumper.js": "1.3.1",
    "enquirer": "2.3.6",
    "execa": "0.2.2",
    "fs-extra": "9.1.0",
    "fs-extra-promise": "1.0.1",
    "fs-jetpack": "2.4.0",
    "git-user-info": "1.0.1",
    "git-user-local": "1.0.0",
    "github-username": "6.0.0",
    "ip": "1.1.5",
    "min-dot": "0.1.1",
    "minimist": "1.2.5",
    "mocha": "6.0.2",
    "mustache": "4.1.0",
    "node-ip": "0.1.2",
    "npm-which": "3.0.1",
    "ora": "5.3.0",
    "please-upgrade-node": "3.2.0",
    "pluralize": "8.0.0",
    "semver": "7.3.4",
    "trash": "7.1.0",
    "update-notifier": "4.1.0",
    "voca": "1.4.0",
    "which": "2.0.2"
  }
}