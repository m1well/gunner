{
  "name": "@codedungeon/gunner",
  "packageName": "gunner",
  "version": "0.0.5",
  "description": "node cli starter framework",
  "tagline": "Crafted with love by Codedungeon",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikeerickson/gunner"
  },
  "contributors": [
    {
      "name": "Mike Erickson",
      "email": "mike.erickson@codedungeon.io",
      "url": "https://github.com/mikeerickson"
    }
  ],
  "main": "index.js",
  "bin": {
    "gunner": "./index.js"
  },
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "lint": "./tasks/lint.sh",
    "test": "mocha ./test/*.test.js",
    "test:watch": "mocha ./test/*.test.js --reporter dot --growl --watch",
    "todo": "node tasks/todo.js",
    "pre-commit": "npm run lint && npm run test"
  },
  "keywords": [
    "cli",
    "codedungeon",
    "framework",
    "javascript",
    "node",
    "starter"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "babel-eslint": "10.0.1",
    "commitlint": "7.5.2",
    "eslint": "5.15.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "leasot": "7.3.4",
    "prettier": "1.16.4"
  },
  "dependencies": {
    "@codedungeon/messenger": "0.6.0",
    "@codedungeon/utils": "0.0.12",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "colors": "1.3.3",
    "enquirer": "2.3.0",
    "fs-extra": "7.0.1",
    "has-flag": "3.0.0",
    "mocha": "6.0.2",
    "voca": "1.4.0",
    "yargs-parser": "13.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
